set xlabel 'temperature'

set label 1 'test 1'

set ylabel 'Energy'
plot \
  'triangle-g-1.dat' u 1:2 title 'exact diagonalization' w lp, \
  'triangle-e-1.dat' u 1:2:(2*$3) title 'SSE' w e, \
  'triangle-p-1.dat' u 1:2:(2*$3) title 'path integral' w e
pause -1

set ylabel 'Specific Heat'
plot \
  'triangle-g-1.dat' u 1:3 title 'exact diagonalization' w lp, \
  'triangle-e-1.dat' u 1:4:(2*$5) title 'SSE' w e, \
  'triangle-p-1.dat' u 1:4:(2*$5) title 'path integral' w e
pause -1

set ylabel 'Magnetization^2'
plot \
  'triangle-g-1.dat' u 1:4 title 'exact diagonalization' w lp, \
  'triangle-e-1.dat' u 1:6:(2*$7) title 'SSE' w e, \
  'triangle-p-1.dat' u 1:6:(2*$7) title 'path integral' w e
pause -1

set ylabel 'Uniform Susceptibility'
plot \
  'triangle-g-1.dat' u 1:5 title 'exact diagonalization' w lp, \
  'triangle-e-1.dat' u 1:8:(2*$9) title 'SSE' w e, \
  'triangle-p-1.dat' u 1:8:(2*$9) title 'path integral' w e
pause -1

set ylabel 'Sign'
plot \
  'triangle-e-1.dat' u 1:10:(2*$11) title 'SSE' w e, \
  'triangle-e-1.dat' u 1:12:(2*$13) title 'SSE (improved)' w e, \
  'triangle-p-1.dat' u 1:10:(2*$11) title 'path integral' w e, \
  'triangle-p-1.dat' u 1:12:(2*$13) title 'path integral (improved)' w e
pause -1

