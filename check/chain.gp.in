set xlabel 'temperature'

set title 'test 1'

set ylabel 'Energy'
plot \
  'chain-g-1.dat' u 1:2 title 'exact diagonalization' w lp, \
  'chain-e-1.dat' u 1:2:(2*$3) title 'SSE' w e, \
  'chain-p-1.dat' u 1:2:(2*$3) title 'path integral' w e
pause -1

set ylabel 'Specific Heat'
plot \
  'chain-g-1.dat' u 1:3 title 'exact diagonalization' w lp, \
  'chain-e-1.dat' u 1:4:(2*$5) title 'SSE' w e, \
  'chain-p-1.dat' u 1:4:(2*$5) title 'path integral' w e
pause -1

set ylabel 'Magnetization^2'
plot \
  'chain-g-1.dat' u 1:4 title 'exact diagonalization' w lp, \
  'chain-e-1.dat' u 1:6:(2*$7) title 'SSE' w e, \
  'chain-p-1.dat' u 1:6:(2*$7) title 'path integral' w e
pause -1

set ylabel 'Uniform Susceptibility'
plot \
  'chain-g-1.dat' u 1:5 title 'exact diagonalization' w lp, \
  'chain-e-1.dat' u 1:8:(2*$9) title 'SSE' w e, \
  'chain-p-1.dat' u 1:8:(2*$9) title 'path integral' w e
pause -1

set ylabel 'Staggered Magnetization^2'
plot \
  'chain-g-1.dat' u 1:6 title 'exact diagonalization' w lp, \
  'chain-e-1.dat' u 1:10:(2*$11) title 'SSE' w e, \
  'chain-p-1.dat' u 1:10:(2*$11) title 'path integral' w e
pause -1

